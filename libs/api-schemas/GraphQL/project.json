{
  "name": "GraphQL-schema",
  "projectType": "library",
  "tags": ["api", "schema", "graphql"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default"],
      "options": {
        "command": "ts-node libs/api-schemas/GraphQL/scripts/validateGqlSchema.js"
      }
    },
    "build": {
      "dependsOn": ["lint"],
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default"],
      "options": {
        "command": "graphql-codegen-esm --config libs/api-schemas/GraphQL/codegen.ts"
      }
    }
  }
}
