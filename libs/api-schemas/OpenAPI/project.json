{
  "name": "OpenAPI-schema",
  "sourceRoot": "libs/api-schemas/OpenAPI",
  "projectType": "library",
  "tags": ["api", "schema", "openapi"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default"],
      "options": {
        "command": "redocly lint --config libs/api-schemas/OpenAPI/redocly.yaml"
      }
    },
    "build": {
      "dependsOn": ["lint"],
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default"],
      "options": {
        "command": "ts-node libs/api-schemas/OpenAPI/scripts/codegenOpenApiSchemaTypes.js"
      }
    }
  }
}
