{
  "name": "fixit-api",
  "version": "1.23.0-next.3",
  "description": "Fixit API services built on NodeJS and Apollo GraphQL.",
  "author": {
    "name": "Trevor Anderson",
    "email": "Trevor@Nerdware.cloud"
  },
  "license": "LicenseRef-LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerdware-LLC/fixit-api.git"
  },
  "private": true,
  "homepage": "https://github.com/Nerdware-LLC/fixit-api#readme",
  "bugs": {
    "url": "https://github.com/Nerdware-LLC/fixit-api/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "docker compose up",
    "start:dist": "API_COMMAND='node ./dist/index.js' docker compose up",
    "build": "rm -rf ./dist 2>/dev/null && swc src --out-dir dist",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage --bail=1 --silent",
    "gql:update-schema-and-types": "npm run gql:fetch-current-schema && npm run gql:codegen",
    "gql:fetch-current-schema": "rover graph fetch fixit@current > fixit@current.graphql",
    "gql:codegen": "graphql-codegen --config codegen.yaml"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "@aws-sdk/client-dynamodb": "^3.391.0",
    "@aws-sdk/client-lambda": "^3.391.0",
    "@aws-sdk/lib-dynamodb": "^3.391.0",
    "@graphql-tools/schema": "^10.0.0",
    "@sentry/node": "^7.64.0",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash.merge": "^4.6.2",
    "stripe": "^10.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@apollo/rover": "^0.18.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-tools/mock": "^9.0.0",
    "@serverless-guru/prettier-plugin-import-order": "^0.4.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.5.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-v8": "^0.34.2",
    "aws-sdk-client-mock": "^3.0.0",
    "eslint": "^8.47.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vitest": "^0.2.8",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2",
    "vitest-github-actions-reporter": "^0.10.0"
  }
}
