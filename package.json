{
  "name": "fixit-api",
  "version": "1.0.0",
  "description": "Fixit API services built on NodeJS and Apollo GraphQL.",
  "author": {
    "name": "Trevor Anderson",
    "email": "Trevor@Nerdware.cloud"
  },
  "license": "LicenseRef-LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerdware-LLC/fixit-api.git"
  },
  "private": "true",
  "homepage": "https://github.com/Nerdware-LLC/fixit-api#readme",
  "bugs": {
    "url": "https://github.com/Nerdware-LLC/fixit-api/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "nodemon ./src/index.ts dotenv_config_path=./src/server/.dev.env",
    "start:prod": "node --no-warnings --loader ./build/loader.js ./build/index.js",
    "build": "rm -rf ./build 2>/dev/null && tsc --project tsconfig.build.json",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' dotenv_config_path=./src/server/.test.env jest",
    "test:watch": "npm run test -- --watch ;        # Re-run tests when associated files change",
    "test:verbose": "npm run test -- --verbose ;    # Get test log output in console"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.142.0",
    "@aws-sdk/client-lambda": "^3.142.0",
    "@aws-sdk/client-s3": "^3.160.0",
    "@aws-sdk/lib-dynamodb": "^3.150.0",
    "@sentry/node": "^7.9.0",
    "@sentry/tracing": "^7.12.1",
    "@types/lodash.merge": "^4.6.7",
    "@types/module-alias": "^2.0.1",
    "apollo-server-core": "^3.10.1",
    "apollo-server-express": "^3.10.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "stripe": "^10.7.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@jest/types": "^29.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/colors": "^1.2.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.7.14",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.21.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.2"
  },
  "_moduleAliases": {
    "@events": "build/events",
    "@graphql": "build/graphql",
    "@lib": "build/lib",
    "@middleware": "build/middleware",
    "@models": "build/models",
    "@server": "build/server",
    "@utils": "build/utils"
  }
}
